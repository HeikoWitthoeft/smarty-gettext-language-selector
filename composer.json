{
  "name": "wazaari/smarty-gettext-language-selector",
  "description": "A DropDown Language Selector which can be set to a Smarty template, and sets gettext environment variables",
  "keywords": [
    "smarty",
    "gettext",
    "language",
    "selection",
    "selector",
    "i18n",
    "internationalization"
  ],
  "homepage": "https://github.com/waza-ari/smarty-gettext-language-selector",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Herrmann",
      "email": "daniel.herrmann1@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "marghoobsuleman/ms-Dropdown",
        "type": "component",
        "version": "dev-master",
        "dist": {
          "url": "https://github.com/marghoobsuleman/ms-Dropdown/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/marghoobsuleman/ms-Dropdown.git",
          "type": "git",
          "reference": "master"
        },
        "extra": {
          "component": {
            "scripts": [
              "js/msdropdown/jquery.dd.js"
            ],
            "styles": [
              "css/msdropdown/dd.css",
              "css/msdropdown/flags.css"
            ],
            "files": [
              "js/msdropdown/jquery.dd.min.js",
              "images/msdropdown/icons/blank.gif",
              "images/msdropdown/icons/flagssprite_small.png",
              "images/msdropdown/skin1/dd_arrow.gif",
              "images/msdropdown/skin1/title-bg.gif"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    }
  ],
  "require": {
    "ext-gettext": "*",
    "php": ">=5.2.1",
    "smarty/smarty": "3.*",
    "components/jquery": "*",
    "marghoobsuleman/ms-Dropdown": "dev-master"
  },
  "suggest": {
    "smarty-gettext": "*"
  },
  "autoload": {
    "files": ["src/SmartyLanguageSelector/block.languageChooser.php"],
    "psr-4": {"SmartyLanguageSelector\\": "src/SmartyLanguageSelector"}
  }
}
